dataset @org.agent.cooking {
  query := @org.agent.cooking.recipe()
  #_[preprocessed=["the recipe"]];

  query := @org.agent.cooking.instruction()
  #_[preprocessed=["the instruction"]];

  query (p_id : Entity(org.agent.cooking:recipe)) :=
  @org.agent.cooking.instruction(recipe=p_id)
  #_[preprocessed=["what's the instruction of ${p_id}"]];

  query (i_id : Entity(org.agent.cooking:ingredient)) :=
  @org.agent.cooking.unit_conversion(ingredient=i_id)
  #_[preprocessed=["convert ${i_id} to"]];
}